{
  "name": "jass",
  "displayName": "jass",
  "description": "目标是扩展成为主流的jass插件。\nQQ群：137199441，提出你宝贵的建议。",
  "publisher": "jass",
  "icon": "static/jass.png",
  "version": "1.6.3",
  "keywords": [
    "jass",
    "warcraft"
  ],
  "engines": {
    "vscode": "^1.37.0"
  },
  "markdown": "github",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naichabaobao/jass"
  },
  "homepage": "https://github.com/naichabaobao/jass",
  "activationEvents": [
    "onLanguage:jass",
    "onLanguage:lua"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jass",
      "properties": {
        "jass.common_j": {
          "type": "string",
          "description": "自定义common.j(reboot)\n(Customize common.j, using the common.j provided by the plug-in by default when not set)"
        },
        "jass.blizzard": {
          "type": "string",
          "description": "自定义blizzard.j(reboot)"
        },
        "jass.common_ai": {
          "type": "string",
          "description": "自定义common.j(reboot)"
        },
        "jass.dzapi": {
          "type": "string",
          "description": "自定义dzapi.j(reboot)"
        },
        "jass.includes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "包含路徑或文件(如：ydwe，japi等,需要重启),插件會找到.j | .ai | .zn\n(Contains a path or file (such as ydwe, japi, etc.) that the plug-in will find .j | .ai | .zn)"
        },
        "jass.cjass.dependents": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "cjass standard library"
        },
        "jass.support.cjass": {
          "type": "boolean",
          "default": false,
          "description": "support cjass(not recommended)"
        },
        "jass.support.zinc": {
          "type": "boolean",
          "default": true,
          "description": "是否支持zinc\n(Whether to support Zinc)"
        },
        "jass.only": {
          "type": "boolean",
          "default": false,
          "description": "only jass model"
        },
        "jass.diagnostic": {
          "type": "boolean",
          "default": false,
          "description": "jass diagnostic, required in only jass model"
        }
      }
    },
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "jass",
          "jass2"
        ],
        "extensions": [
          ".j",
          ".ai",
          ".jass",
          ".zn"
        ],
        "configuration": "./src/jass-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.jass",
        "language": "jass",
        "path": "./src/jass.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jass",
        "path": "./src/snippets.json"
      }
    ],
    "documentFormattingProvider": true,
    "productIconTheme": [
      {
        "id": "jass",
        "label": "jass",
        "path": "./src/jass-product-icon-theme.json"
      }
    ],
    "enableProposedApi": false
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.37.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.4",
    "vsce": "^1.103.1",
    "vscode-test": "^1.2.2"
  },
  "bugs": {
    "url": "https://github.com/naichabaobao/jass/issues"
  },
  "dependencies": {},
  "author": "家猫hoo",
  "license": "GPL"
}
