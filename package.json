{
  "name": "jass",
  "displayName": "jass",
  "description": "jass插件,QQ群:786204376",
  "publisher": "jass",
  "icon": "static/jass.png",
  "version": "1.8.44",
  "keywords": [
    "jass",
    "vjass",
    "zinc",
    "warcraft"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "markdown": "github",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naichabaobao/jass"
  },
  "homepage": "https://github.com/naichabaobao/jass",
  "activationEvents": [
    "onLanguage:jass"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jass",
      "properties": {
        "jass.lua.dependents": {
          "type": "array",
          "items": {
            "type": "(Deprecation) string"
          },
          "description": "(Deprecation) '.lua' file library"
        },
        "jass.support.lua": {
          "type": "boolean",
          "default": false,
          "description": "(Deprecation) Support lua, Cooperate with 'jass.lua.dependents' consumption"
        },
        "jass.cjass.dependents": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "(Deprecation) cjass standard library"
        },
        "jass.support.cjass": {
          "type": "boolean",
          "default": false,
          "description": "(Deprecation) support cjass(not recommended)"
        },
        "jass.diagnostic": {
          "type": "boolean",
          "default": true,
          "description": "(Test) diagnostic support,does not support including Lua and pre-processing code"
        },
        "jass.literal": {
          "type":"boolean",
          "default": false,
          "dependencies": "Whether to prompt for literal values (string, number, mark)"
        }
      }
    },
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "jass",
          "jass2"
        ],
        "extensions": [
          ".j",
          ".ai",
          ".jass",
          ".zn"
        ],
        "configuration": "./static/jass-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.jass",
        "language": "jass",
        "path": "./static/jass.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jass",
        "path": "./static/snippets.json"
      }
    ],
    "documentFormattingProvider": true,
    "enableProposedApi": false,
    "codeLensProvider": {
      "resolveProvider": "true"
    },
    "commands": [
    ],
    "menus": {
    }
  },
  "capabilities": {
    "documentSymbolProvider": "true",
    "workspaceSymbolProvider": "true"
  },
  "scripts": {
    "compile": "tsc  -p ./",
    "compilep": "tsc  -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test:all": "node out/boot/provider/test-runner.js all",
    "test:suite": "node out/boot/provider/test-runner.js suite",
    "test:single": "node out/boot/provider/test-runner.js test",
    "test:report": "node out/boot/provider/test-runner.js report",
    "package": "vsce package --no-dependencies",
    "publish:skip-duplicate": "vsce publish --no-dependencies --skip-duplicate",
    "publish": "vsce publish --no-dependencies",
    "prepublish": "vsce publish --pre-release --no-dependencies --skip-duplicate"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.24.0",
